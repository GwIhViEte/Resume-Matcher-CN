{
  "version": 2,
  "builds": [
    {
      "src": "next.config.ts",
      "use": "@vercel/next"
    },
    {
      "src": "../backend/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api_be/:path*",
      "destination": "/../backend/app/main.py"
    },
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ]
}
